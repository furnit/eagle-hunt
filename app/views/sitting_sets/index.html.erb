<p id="notice"><%= notice %></p>

<h1>Sitting Sets</h1>

<table>
  <thead>
    <tr>
      <th>Count</th>
      <th>Details</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sitting_sets.each do |sitting_set| %>
      <tr>
        <td><%= sitting_set.count %></td>
        <td><%= sitting_set.details %></td>
        <td><%= sitting_set.comment %></td>
        <td><%= link_to 'Show', sitting_set %></td>
        <td><%= link_to 'Edit', edit_sitting_set_path(sitting_set) %></td>
        <td><%= link_to 'Destroy', sitting_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sitting Set', new_sitting_set_path %>
