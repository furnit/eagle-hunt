<% form_id = '' %>
<%= remote_form(admin_pricing_transit) do |f| -%>
  <% form_id = f.options[:html][:id] %>
  <div  class='block'>
      <div class="field">
        <%= f.text_field :admin_workshop_workshop_id %>
      </div>

      <div class="field">
        <%= f.text_field :state_id %>
      </div>

      <div class="field">
        <%= f.number_field :price %>
      </div>

      <div class='clearfix'></div>
  </div>

  <div class="actions field pull-left">
    <%= f.submit submit_text,  data: { disable_with: "درحال ثبت اطلاعات..." }, class: 'btn btn-primary' %>
  </div>
  <div class='clearfix'></div>
<% end %>