<p id="notice"><%= notice %></p>

<h1>Admin Workshops</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>State</th>
      <th>Address</th>
      <th>User</th>
      <th>Phone number</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_workshops.each do |admin_workshop| %>
      <tr>
        <td><%= admin_workshop.name %></td>
        <td><%= admin_workshop.state %></td>
        <td><%= admin_workshop.address %></td>
        <td><%= admin_workshop.user %></td>
        <td><%= admin_workshop.phone_number %></td>
        <td><%= admin_workshop.comment %></td>
        <td><%= link_to 'Show', admin_workshop %></td>
        <td><%= link_to 'Edit', edit_admin_workshop_path(admin_workshop) %></td>
        <td><%= link_to 'Destroy', admin_workshop, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Workshop', new_admin_workshop_path %>
