<p id="notice"><%= notice %></p>

<h1>Admin Furniture Color Brands</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_furniture_color_brands.each do |admin_furniture_color_brand| %>
      <tr>
        <td><%= admin_furniture_color_brand.name %></td>
        <td><%= admin_furniture_color_brand.comment %></td>
        <td><%= link_to 'Show', admin_furniture_color_brand %></td>
        <td><%= link_to 'Edit', edit_admin_furniture_color_brand_path(admin_furniture_color_brand) %></td>
        <td><%= link_to 'Destroy', admin_furniture_color_brand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Furniture Color Brand', new_admin_furniture_color_brand_path %>
