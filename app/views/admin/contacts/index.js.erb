<% js = escape_javascript( render 'list' ) %>
$("#filterrific_results").html("<%= js %>");

<% if @filterrific.instance_variable_defined?(:@search_query) and not @filterrific.search_query.to_s.empty? %>
$("#filterrific_results").highlight("<%= escape_javascript(@filterrific.search_query.to_s) %>")
<% end %>